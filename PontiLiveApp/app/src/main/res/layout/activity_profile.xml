<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.ProfileActivity"
    >


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:fadingEdgeLength="@dimen/cardview_compat_inset_shadow"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/menu_rectanguloi" />

    <ImageButton
        android:id="@+id/chatButton"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/round_button"
        android:src="@drawable/menu_boton_chat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.141"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="Accesos a mensajes"
        />

    <ImageButton
        android:id="@+id/mapButton"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/round_button"
        android:src="@drawable/menu_boton_mapal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="Accesos mapa principal"/>

    <ImageButton
        android:id="@+id/profileButton"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/round_button"
        android:src="@drawable/menu_botonp_perfil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.851"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="Accesos a perfil"/>

    <ImageView
        android:id="@+id/configButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/config" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="137dp"
        android:layout_height="138dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/perfil_elipse" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_medium"
        android:text="Juana Camargo"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/raleway_normal"
        android:text="ComunicaciÃ³n Social"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/botonNaranja"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <ImageButton
        android:id="@+id/botonNaranja"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:background="@android:color/transparent"
        android:contentDescription="Descripcion naranja"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/botonBeige"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/perfil_boton_naranja" />

    <ImageButton
        android:id="@+id/botonBeige"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:background="@android:color/transparent"
        android:contentDescription="Descripcion beige"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/botonNaranja"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/perfil_boton_beige" />

    <ImageButton
        android:id="@+id/arca"
        android:layout_width="170dp"
        android:layout_height="107dp"
        android:background="@android:color/transparent"
        android:contentDescription="Acceder a primer grupo"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/artes"
        app:layout_constraintEnd_toStartOf="@+id/ing"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/arca" />

    <ImageButton
        android:id="@+id/ing"
        android:layout_width="170dp"
        android:layout_height="107dp"
        android:background="@android:color/transparent"
        android:contentDescription="Acceder a segundo grupo"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/arqui"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/arca"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/inge" />

    <ImageButton
        android:id="@+id/artes"
        android:layout_width="170dp"
        android:layout_height="107dp"
        android:background="@android:color/transparent"
        android:contentDescription="Acceder a tercer grupo"
        app:layout_constraintBottom_toTopOf="@+id/morePlaces"
        app:layout_constraintEnd_toStartOf="@+id/arqui"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arca"
        app:srcCompat="@drawable/artes" />

    <ImageButton
        android:id="@+id/arqui"
        android:layout_width="170dp"
        android:layout_height="107dp"
        android:background="@android:color/transparent"
        android:contentDescription="Acceder a cuarto grupo"
        app:layout_constraintBottom_toTopOf="@+id/morePlaces"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/artes"
        app:layout_constraintTop_toBottomOf="@+id/ing"
        app:srcCompat="@drawable/arqui" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_medium"
        android:text="Top Places"
        android:textColor="#646363"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/arca"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonNaranja" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.71"
        app:srcCompat="@drawable/perfil_imagen" />

    <ImageButton
        android:id="@+id/morePlaces"
        android:layout_width="45dp"
        android:layout_height="33dp"
        android:background="@drawable/round_button"
        android:src="@drawable/ellipsis_horizontal_circle"
        app:layout_constraintBottom_toTopOf="@+id/chatButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arqui" />

</androidx.constraintlayout.widget.ConstraintLayout>