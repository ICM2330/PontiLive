    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="65dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/chat3"
        app:layout_constraintEnd_toEndOf="@+id/chat3"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="@+id/chat3"
        app:layout_constraintTop_toTopOf="@+id/chat3"
        app:srcCompat="@drawable/pontigomitas" />

</androidx.constraintlayout.widget.ConstraintLayout>